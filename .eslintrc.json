{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
      "@typescript-eslint"
  ],
  "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
      "semi": 0,
      "no-extra-semi": 2,
      "no-extra-boolean-cast": 0,
      "quotes": [2, "single", "avoid-escape"]
  }
}
